
<div  class="jumbotron">
    <h1> Returning User?</h1>
    <h1>Log In</h1>
    <p>New Users Sign Up Here: <%= link_to "Sign Up", new_user_path %> </p>
</div>


<%= form_for :login, {url: login_path} do |f| %>
  <form>
    <div class="form-group">
      <label for="exampleInputEmail1">Email address</label>
      <%= f.email_field :email, placeholder: 'email', class:"form-control", id:"exampleInputEmail1"  %>
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Password</label>
      <%= f.password_field :password, placeholder: "password", class:"form-control", id:"exampleInputPassword1" %>
    </div>

<%= f.submit "Log In", class: "btn btn-primary" %>
  </form>

<% end %>
