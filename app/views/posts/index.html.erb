
<%= render 'nav' %>


<div id="all-posts" class="container">
  <h1>All Posts</h1>
    <% @posts.each do |p| %>

    <div id="donut-post">
      <div id="donut-image">
      <%= image_tag(p.image(:medium)) %>
    </div>

    <div id="donut-caption">
      <%= p.caption %>
    </div>
    <p> <%= link_to "Show", post_path(p.id) %> </p>
  </div>

    <% end %>
</div>
