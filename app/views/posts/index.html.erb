<%= render 'nav' %>
<div class="all-posts" class="container">
    <% @posts.each do |p| %>
      <div class="donut-post">
        <div id="donut-image">
        <%= image_tag(p.image(:medium)) %>
        </div>
        <div id="donut-caption">
          <%= p.caption %>
          <p>Created by: <%= link_to p.user.name, user_path(p.user) %> </p>
        </div>
      <p> <%= link_to "Show", post_path(p.id) %> </p>
    </div>
  <% end %>
</div>
